{
    "name": "n8n-nodes-image-orientation",
    "version": "0.1.0",
    "description": "An n8n node that uses Tesseract.JS to determine the orientation of an image based on the text it contains.",
    "license": "MIT",
    "homepage": "https://github.com/Neremis/n8n-nodes-image-orientation",
    "keywords": [
        "n8n-community-node-package",
        "orientation",
        "n8n-community-node",
        "n8n",
        "tesseract",
        "ocr",
        "osd",
        "image",
        "rotation"
    ],
    "author": {
        "name": "Neremis"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Neremis/n8n-nodes-image-orientation"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": false,
        "credentials": [],
        "nodes": [
            "dist/nodes/ImageOrientation/ImageOrientation.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "tesseract.js": "6.0.1",
        "@tesseract.js-data/osd": "1.0.0"
    }
}
